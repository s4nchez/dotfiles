[push]
	default = current
[branch]
	autosetuprebase = always
[core]
	editor = subl -nw
	autocrlf = input
	ignorecase = false
[alias]
	l = "log --oneline --decorate --color --graph"
	up = pull --rebase
	sup = "!git stash; git pull --rebase; git stash pop"
	out = "log @{u}.. --oneline --decorate --color --graph"
	in = "!git remote update -p; git log ..@{u} --oneline --decorate --color --graph"
	st = status
